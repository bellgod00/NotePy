   "source": [
    "# 3과 5의 배수를 모두 더하기\n",
    "\n",
    "result = 0\n",
    "\n",
    "for n in range(1, 1000):\n",
    "    if n % 3 == 0 or n % 5 == 0:\n",
    "        result += n\n",
    "        print(n, end=' ')\n",
    "\n",
    "print(f\"\\n합계 : {result}\")"
   ]
  }
